{
  "name": "tactic",
  "description": "Promise utilities for just the worst people",
  "version": "0.0.2",
  "author": "Chris Andrejewski <christopher.andrejewski@gmail.com>",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bugs": {
    "url": "https://github.com/andrejewski/tactic/issues"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.11.6",
    "del": "^2.2.1",
    "fixpack": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^2.5.1",
    "nyc": "^10.2.0",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "source-map-support": "^0.4.2",
    "standard": "^10.0.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/andrejewski/tactic#readme",
  "keywords": [
    "node",
    "npm",
    "package",
    "starter"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrejewski/tactic.git"
  },
  "scripts": {
    "build": "gulp",
    "lint": "standard --fix && fixpack",
    "prepublish": "npm run build && npm run test",
    "report": "NODE_ENV=nyc nyc npm test",
    "test": "npm run lint && ava",
    "watch": "gulp watch"
  }
}
